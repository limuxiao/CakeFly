<view class="view-banner">

  <swiper class="swiper-banner" indicator-dots="true" duration="1000" autoplay="true" interval="5000"
   indicator-active-color="#fff" indicator-dots="true" indicator-color="rgba(0, 0, 0, .3)" circular="false">

    <block wx:for="{{imgUrls}}" wx:key="*this" >

      <swiper-item>
         <image class="img-banner-content" src="{{item}}" bindload="onLoadImage"/> 
      </swiper-item>

      
    
    </block>

  </swiper>

</view>