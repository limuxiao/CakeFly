.view-entry{
  width: 100%; height: 160rpx; display: table;
}

.view-entry-item{
  width: 25%; height: 100%; background-color: #fff; display: table-cell; position: relative;
}

.view-entry-icon{
  width: 60%; height: 60%; padding: 4rpx; margin: 4rpx auto;
}

.img-entry-icon{
  width: 100%; height: 100%; margin: 4rpx auto; border-radius: 50%;
}

.view-entry-text{
  width: 100%; height: auto; position: absolute;
  bottom: 10rpx;display: flex; justify-content: center; color: #ff0ff0; font-size: 32rpx;
}
